FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue epsilon 1
);

regions
(
    zoneToCell
    {
        name "laminar"; 
        
	fieldValues
        (
            volScalarFieldValue epsilon 0
        );
    }
);


